{
  "name": "azure_devops_pullrequests",
  "version": "2.7.0",
  "description": "Azure devops chrome extension",
  "scripts": {
    "dev": "webpack-dev-server",
    "start": "webpack --watch --progress",
    "build:gecko": "cross-env BROWSER=gecko yarn _build",
    "build:chrome": "cross-env BROWSER=chrome yarn _build",
    "_build": "yarn webpack:production && yarn zip",
    "build": "yarn build:gecko && yarn build:chrome && yarn zip:source",
    "webpack:production": "cross-env NODE_ENV=production webpack --progress",
    "zip": "node ./zip",
    "zip:source": "node ./zip-source",
    "upgrade": "yarn config set save-prefix \"\" && yarn && yarn upgrade-interactive --latest"
  },
  "keywords": [
    "azure",
    "devops",
    "vsts",
    "rest",
    "api",
    "pullRequest",
    "pr"
  ],
  "author": "RÃ©mi Fruteau de Laclos <remi.fruteau@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "lodash": "4.17.20"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-transform-runtime": "7.12.1",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-modules": "0.1.4",
    "@babel/register": "7.12.1",
    "@babel/runtime": "7.12.5",
    "@fortawesome/fontawesome-free": "5.15.1",
    "@material/select": "4.0.0",
    "angular": "1.8.2",
    "babel-loader": "8.2.1",
    "babel-plugin-angularjs-annotate": "0.10.0",
    "babel-plugin-lodash": "3.3.4",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "6.3.0",
    "cross-env": "7.0.2",
    "css-loader": "5.0.1",
    "detect-browser": "5.2.0",
    "file-loader": "6.2.0",
    "html-loader": "1.3.2",
    "html-webpack-plugin": "4.5.0",
    "mini-css-extract-plugin": "1.3.0",
    "moment": "2.29.1",
    "node-sass": "7.0.0",
    "optimize-css-assets-webpack-plugin": "5.0.4",
    "sass-loader": "10.1.0",
    "speed-measure-webpack-plugin": "1.3.3",
    "style-loader": "2.0.0",
    "terser-webpack-plugin": "5.0.3",
